package ch01;

public class Datatype {
	public static void main(String[] args) {
		int top10 = 10; // 영문 뒤 숫자는 가능
		//int 10top = 10; // 앞이 숫자는 불가능
		//int break = 10; // 자바에서 사용하는 예약어는 사용불가능 (boolean, while, for, break 등등)
		//int * = 10; // 특수문자도 사용 불가능
		int $ = 10; 
		System.out.println($);
		
		
	}//end of main
}//end of class
