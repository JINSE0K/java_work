package ch01;

public class DataTypeTest {
	public static void main(String[] args) {
		// 1. 정수 550을 name1 이름으로 변수에 담아 보세요
		int name1 = 550;
		
		// 2. 정수 22억을 name2 이름으로 변수에 담아 보세요
		int name2_1 = 2200000000; // < 안됌
		long name2_2 = 22000000000L; //가능 정수 자료형이 Long 타입으로 변환해줌
		
		// 3. 문자 A를 name3 이름으로 담아 보세요
		char name3 = 'A';
		
		// 4. 실수 float 타입으로 0.12345678을 name4 이름으로 변수에 담아 보세요.
		float name4 = 0.12345678; // < 안됌
		float name4_1 = 0.12345678F; //가능 뒤에 접미사 F붙혀줌
		
		
		// 5. 실수 double 타입으로 200.0035을 name5 이름으로 변수에 담아 보세요.
		double name5 = 200.0035; // < 그냥 쓰면 됌 제일 많이 씀
		
		// 6. 논리 자료형 isOk 이름의 변수를 선언해 보세요.
		
			boolean isOk; // 변수 선언만함 초기화는 안함
		// 7. isOk 변수에 값을 false로 초기화 해보세요.
			isOk = false;
		// 8. isOk 변수에 값을 true로 변경해 보세요.
			isOk = true;
		// 9. 변수란 무엇인지 서술해 보세요.
			//변수란 언제든지 변할 수 있는 수를 말함
		// 10. 기본 데이터 타입에 종류 8가지를 서술해 보세요.
	}
}
